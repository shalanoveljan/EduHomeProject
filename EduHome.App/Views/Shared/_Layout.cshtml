@inject ISettingService settingService

@{
    SettingGetDto setting = await settingService.GetAsync(18);
}
@if (!string.IsNullOrWhiteSpace((string)TempData["noSendEmail"]))
{
    <div style="display:none" id="message">@TempData["noSendEmail"]</div>
}
@if (!string.IsNullOrWhiteSpace((string)TempData["SendEmail"]))
{
    <div style="display:none" id="message">@TempData["SendEmail"]</div>
}
@if (!string.IsNullOrWhiteSpace((string)TempData["login"]))
{
    <div style="display:none" id="message">@TempData["login"]</div>
}
@if (!string.IsNullOrWhiteSpace((string)TempData["update"]))
{
    <div style="display:none" id="message">@TempData["update"]</div>
}
@if (!string.IsNullOrWhiteSpace((string)TempData["logout"]))
{
    <div style="display:none" id="message">@TempData["logout"]</div>
}


<!doctype html>
