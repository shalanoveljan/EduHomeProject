    <!-- Header Area Start -->
    <header class="top">
        <div class="header-top">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-sm-8 col-xs-12">
                        <div class="header-top-left">
                            <p>
                                HAVE ANY QUESTION ? @setting.PhoneNumber
                            </p>
                        </div>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="header-top-right text-right">
                            <ul>
                                @if (User.Identity.IsAuthenticated && User.IsInRole("User"))
                                {
                                    <li class="nav-item submenu dropdown">
                                        <a asp-controller="identity" asp-action="update" class="nav-link dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true"
                                       aria-expanded="false">@User.Identity.Name</a>
                                    </li>
                                            <li class="nav-item"><a class="nav-link" asp-controller="identity" asp-action="Logout">Logout</a></li>
                                            <li class="nav-item"><a class="nav-link" asp-controller="identity" asp-action="update">UserInfo</a></li>
                                   
                                }
                                else
                                {
                                   
                                        
                                            <li class="nav-item"><a class="nav-link" asp-controller="identity" asp-action="login">Login</a></li>
                                    
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-area two header-sticky">
            <div class="container">
                <div class="row">
                    <div class="col-md-3 col-sm-3 col-xs-6">
                        <div class="logo">
                            <a asp-controller="Home" asp-action="index"><img src="~/assets/img/logo/@setting.Logo" alt="eduhome" /></a>
                        </div>
                    </div>
                    <div class="col-md-9 col-sm-9 col-xs-6">
                        <div class="content-wrapper text-right">
                            <!-- Main Menu Start -->
                            <div class="main-menu">
                                <nav>
                                    <ul>
                                        <li controller="home">
                                            <a asp-controller="Home" asp-action="index">Home</a>
                                        </li>
                                        <li class="nav-item" controller="about"><a asp-controller="About" asp-action="index">About</a></li>
                                        <li class="nav-item" controller="course"><a asp-controller="Course" asp-action="index">Courses</a> </li>
                                        <li class="nav-item" controller="event"><a asp-controller="Event" asp-action="index">Event</a> </li>
                                        <li class="nav-item" controller="teacher"><a asp-controller="Teacher" asp-action="index">Teacher</a> </li>
                                        <li class="nav-item" controller="blog"><a asp-controller="Blog" asp-action="index">Blog</a> </li>
                                        <li class="nav-item" controller="contact"><a asp-controller="Contact" asp-action="index">Contact</a> </li>
                                       
                                    </ul>
                                </nav>
                            </div>
                            <!--Search Form Start-->
                            <div class="search-btn">
                                <ul data-toggle="dropdown" class="header-search search-toggle">
                                    <li class="search-menu">
                                        <i class="fa fa-search"></i>
                                    </li>
                                </ul>
                                <div class="search">
                                    <div class="search-form">
                                        <form id="search-form" action="#">
                                            <input type="search" placeholder="Search here..." name="search" />
                                            <button type="submit">
                                                <span><i class="fa fa-search"></i></span>
                                            </button>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <!--End of Search Form-->
                            <!-- Main Menu End -->
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="mobile-menu hidden-lg hidden-md hidden-sm"></div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- Header Area End -->

    @RenderBody()

    <!-- Subscribe Start -->
    <div class="subscribe-area pt-60 pb-70">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <div class="subscribe-content section-title text-center">
                        <h2>subscribe our newsletter</h2>
                        <p>I must explain to you how all this mistaken idea </p>
                    </div>
                    <div class="newsletter-form mc_embed_signup">
                        <form asp-action="sendemail" asp-controller="home" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
                            <div id="mc_embed_signup_scroll" class="mc-form">
                                <div class="form-group">
                                    <label for="email">Subject</label>
                                    <input type="email" name="email" id="email" class="form-control">
                                    <span class="text text-danger"></span>
                                </div>
                                <button id="mc-embedded-subscribe" class="default-btn" type="submit" name="subcribe"><span>subscribe</span></button>
                            </div>
                        </form>
                        <!-- mailchimp-alerts Start -->
                        <div class="mailchimp-alerts">
                            <div class="mailchimp-submitting"></div><!-- mailchimp-submitting end -->
                            <div class="mailchimp-success"></div><!-- mailchimp-success end -->
                            <div class="mailchimp-error"></div><!-- mailchimp-error end -->
                        </div>
                        <!-- mailchimp-alerts end -->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Subscribe End -->
    <!-- Footer Start -->
    <footer class="footer-area">
        <div class="main-footer">
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-sm-6 col-xs-12">
                        <div class="single-widget pr-60">
                            <div class="footer-logo pb-25">
                                <a href="~/assets/index.html"><img src="~/assets/img/logo/footer-logo.png" alt="eduhome"></a>
                            </div>
                            <p>I must explain to you how all this mistaken idea of denoung pleure and praising pain was born and give you a coete account of the system. </p>
                            <div class="footer-social">
                                <ul>
                                    <li><a href="@setting.FacebookUrl"><i class="fa-brands fa-facebook"></i></a></li>
                                    <li><a href="@setting.TwitterUrl"><i class="fa-brands fa-twitter"></i></a></li>
                                    <li><a href="@setting.PinterestUrl"><i class="fa-brands fa-pinterest"></i></a></li>
                                    <li><a href="@setting.VimeoUrl"> <i class="fa-brands fa-vimeo"></i></a></li>
                                </ul>
                            </div>
                        </div>     
                    </div>
                    <div class="col-md-3 col-sm-6 col-xs-12">
                        <div class="single-widget">
                            <h3>information</h3>
                            <ul>
                                <li><a href="~/assets/#">addmission</a></li>
                                <li><a href="~/assets/#">Academic Calender</a></li>
                                <li><a href="~/assets/event.html">Event List</a></li>
                                <li><a href="~/assets/#">Hostel &amp; Dinning</a></li>
                                <li><a href="~/assets/#">TimeTable</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-2 col-sm-6 col-xs-12">
                        <div class="single-widget">
                            <h3>useful links</h3>
                            <ul>
                                <li><a href="~/assets/course.html">our courses</a></li>
                                <li><a href="~/assets/about.html">about us</a></li>
                                <li><a href="~/assets/teacher.html">teachers &amp; faculty</a></li>
                                <li><a href="~/assets/#">teams &amp; conditions</a></li>
                                <li><a href="~/assets/event.html">our events</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6 col-xs-12">
                        <div class="single-widget">
                            <h3>get in touch</h3>
                            <p>Your address goes here, Street<br>City, @setting.Address</p>
                            <p>@setting.PhoneNumber</p>
                            <p>@setting.Email</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer-bottom text-center">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12">
                        <p>Copyright © <a href="~/assets/#" target="_blank">HasTech</a> 2017. All Right Reserved By Hastech.</p>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- Footer End -->
    <script>
        const values = location.pathname.trim().split('/')
        const navItems = document.querySelectorAll(".nav-item");


        navItems.forEach(item => {
            const value = item.getAttribute("controller");
            if (values[1] == "" && value == "home") {
                item.classList.add("active")
            } else if (values[1] === value) {
                item.classList.add("active")
            }
        })</script>
